let SessionLoad = 1
if &cp | set nocp | endif
let s:cpo_save=&cpo
set cpo&vim
imap <C-Tab> :tabnexti 
imap <C-S-Tab> :tabpreviousi 
nmap  :tabnew 
map Q gq
nmap gx <Plug>NetrwBrowseX
map <C-Tab> :tabnext 
map <C-S-Tab> :tabprevious 
nnoremap <silent> <Plug>NetrwBrowseX :call netrw#NetBrowseX(expand("<cWORD>"),0)
imap  
imap  :tabnew
abbr c2 *****************************************/
abbr c1 /*****************************************
let &cpo=s:cpo_save
unlet s:cpo_save
