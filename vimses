let s:cpo_save=&cpo
set cpo&vim
imap <C-S-Tab> :tabpreviousi 
imap <C-Tab> :tabnexti 
imap <F2> :tabnexti 
imap <F1> :tabpreviousi 
nmap  :tabnew 
map <silent>  :if expand("%") == "":browse confirm w!:else:confirm w!:endif
map Q gq
nmap gx <Plug>NetrwBrowseX
map <C-S-Tab> :tabprevious 
map <C-Tab> :tabnext 
map <F2> :tabnext 
map <F1> :tabprevious 
nnoremap <silent> <Plug>NetrwBrowseX :call netrw#NetBrowseX(expand("<cWORD>"),0)
imap  
imap  :tabnew
abbr c3 /***************************************/
abbr c2 *****************************************/
abbr c1 /*****************************************
